{
  "name": "subpar-dev",
  "build": {
    "dockerfile": "Dockerfile"
  },
  "runArgs": [
    "--net=host",
    "--env", "DISPLAY=host.docker.internal:0"
  ],
  "extensions": [
    "ms-vscode.cpptools",
    "ms-vscode.cmake-tools",
    "ms-python.python",
    "ms-toolsai.jupyter",
    "bierner.markdown-preview-github-styles"
  ],
  "settings": {
    "terminal.integrated.defaultProfile.linux": "bash"
  }
}
