<?xml version="1.0" ?>
<sdf version="1.10">
  <world name="default">
    <light type="directional" name="sun">
      <cast_shadows>true</cast_shadows>
      <pose>0 0 10 0 0 0</pose>
      <diffuse>1 1 1 1</diffuse>
      <specular>0.5 0.5 0.5 1</specular>
      <attenuation>
        <range>1000</range>
        <constant>0.9</constant>
        <linear>0.01</linear>
        <quadratic>0.001</quadratic>
      </attenuation>
      <direction>-0.5 0.1 -0.9</direction>
    </light>
    <model name="joint_controller_demo">
      <pose>0 0 0 0 0 0</pose>
      <link name="base_link">
        <pose>0.0 0.0 0.0 0 0 0</pose>
        <inertial>
          <inertia>
            <ixx>2.501</ixx>
            <ixy>0</ixy>
            <ixz>0</ixz>
            <iyy>2.501</iyy>
            <iyz>0</iyz>
            <izz>5</izz>
          </inertia>
          <mass>120.0</mass>
        </inertial>

        <collision name="base_collision">
          <pose>0.0 0.0 0.0 0 0 0</pose>
          <geometry>
            <box>
              <size>0.5 0.5 0.01</size>
            </box>
          </geometry>
        </collision>
      </link>
      <link name="rotor">
        <pose>0.0 0.0 0 0 0 0</pose>
        <inertial>
          <inertia>
            <ixx>0.032</ixx>
            <ixy>0</ixy>
            <ixz>0</ixz>
            <iyy>0.032</iyy>
            <iyz>0</iyz>
            <izz>0.00012</izz>
          </inertia>
          <mass>0.6</mass>
        </inertial>
        <visual name="visual">
          <geometry>
            <box>
              <size>0.5 0.1 0.05</size>
            </box>
          </geometry>
          <material>
            <ambient>0.2 0.8 0.2 1</ambient>
            <diffuse>0.8 0 0 1</diffuse>
          </material>
          <transparency>0.9</transparency>
        </visual>
        <collision name="collision">
          <geometry>
            <box>
              <size>0.5 0.1 0.05</size>
            </box>
          </geometry>
        </collision>
      </link>
 
          <link name="rotor_2">
        <!-- <pose>0.0 0.2 0.4 1.5708 0 0</pose> -->
        <pose>0 0.25 0.25 1.5708 0 0</pose>
          <inertial>
            <mass>1.0</mass>
            <inertia>
              <ixx>0.05</ixx>
              <iyy>0.05</iyy>
              <izz>0.05</izz>
              <ixy>0</ixy><ixz>0</ixz><iyz>0</iyz>
            </inertia>
          </inertial>
        <visual name="visual">
          <geometry>
            <box>
              <size>0.5 0.1 0.05</size>
            </box>
          </geometry>
          <material>
            <ambient>0.2 0.8 0.2 1</ambient>
            <diffuse>0.8 0 0 1</diffuse>
          </material>
          <transparency>0.9</transparency>
        </visual>
        <collision name="collision_2">
          <geometry>
            <box>
              <size>0.5 0.1 0.05</size>
            </box>
          </geometry>
        </collision>
      </link>

       <link name="thruster">
        <!-- <pose>0.0 0.2 0.4 1.5708 0 0</pose> -->
        <pose>0 0 0.25 0 0 0</pose>
          <inertial>
            <mass>0</mass>
            <inertia>
              <ixx>0.05</ixx>
              <iyy>0.05</iyy>
              <izz>0.05</izz>
              <ixy>0</ixy><ixz>0</ixz><iyz>0</iyz>
            </inertia>
          </inertial>
        <visual name="visual">
          <geometry>
            <sphere>
              <radius>0.25</radius>
            </sphere>
          </geometry>
          <material>
            <ambient>0.2 0.8 0.2 1</ambient>
            <diffuse>0.8 0 0 1</diffuse>
          </material>
        </visual>
        <collision name="collision_3">
          <geometry>
            <sphere>
              <radius>0.25</radius>
            </sphere>
          </geometry>
        </collision>
      </link>
             <link name="thruster_direction_marker">
        <!-- <pose>0.0 0.2 0.4 1.5708 0 0</pose> -->
        <pose>0 0 0.5 0 0 0</pose>
          <inertial>
            <mass>0</mass>
            <inertia>
              <ixx>0.05</ixx>
              <iyy>0.05</iyy>
              <izz>0.05</izz>
              <ixy>0</ixy><ixz>0</ixz><iyz>0</iyz>
            </inertia>
          </inertial>
        <visual name="visual">
          <geometry>
            <cylinder>
              <radius>0.05</radius>
              <length>0.5</length>
            </cylinder>
          </geometry>
          <material>
            <ambient>0.2 0.8 0.2 1</ambient>
            <diffuse>0.8 0 0 1</diffuse>
          </material>
        </visual>
        <collision name="collision_4">
          <geometry>
            <cylinder>
              <radius>0.05</radius>
              <length>0.5</length>
            </cylinder>
          </geometry>
        </collision>
      </link>

      <joint name="world_fixed" type="fixed">
        <parent>world</parent>
        <child>base_link</child>
      </joint>
 
        <joint name="j1" type="revolute">
          <pose>0 0 0 0 0 0</pose>
          <parent>base_link</parent>
          <child>rotor</child>
          <axis>
            <xyz>0 0 1</xyz>
          </axis>
        </joint>

        <joint name="j2" type="revolute">
        <pose>0 0 0 0 0 0</pose>
        <parent>rotor</parent>
        <child>rotor_2</child>
        <axis>
          <xyz> 0 0 1</xyz>
        </axis>
      </joint>

       <joint name="j3" type="fixed">
          <pose>0 0 0 0 0 0</pose>
          <parent>rotor_2</parent>
          <child>thruster</child>
         </joint> 


        <joint name="j4" type="fixed">
          <pose>0 0 0 0 0 0</pose>
          <parent>thruster</parent>
          <child>thruster_direction_marker</child>
         </joint> 
        <plugin
          filename="gz-sim-joint-position-controller-system"
          name="gz::sim::systems::JointPositionController">
          <joint_name>j1</joint_name>
          <topic>topic_name</topic>
    <p_gain>1</p_gain>
          <i_gain>0.1</i_gain>
          <d_gain>0.5</d_gain>
          <i_max>1</i_max>
          <i_min>-1</i_min>
          <cmd_max>1000</cmd_max>
          <cmd_min>-1000</cmd_min>
          </plugin>
              <plugin
      filename="gz-sim-joint-position-controller-system"
      name="gz::sim::systems::JointPositionController">
      <joint_name>j2</joint_name>
      <topic>topic_j2</topic>
<p_gain>1</p_gain>
      <i_gain>0.1</i_gain>
      <d_gain>0.5</d_gain>
      <i_max>1</i_max>
      <i_min>-1</i_min>
      <cmd_max>1000</cmd_max>
      <cmd_min>-1000</cmd_min>
      </plugin>
      <plugin
        name="subpar::SubparPublisherPlugin"
        filename="libSubparDronePlugin.so"/>

<!-- <plugin name="subpar::SubparKeyInputPlugin" filename="libSubparKeyInputPlugin.so"/> -->
<!-- <plugin filename="libSubparKeyGuiPlugin.so" name="subpar::KeyGuiPlugin"/> -->

    </model>
    
  </world>


</sdf>